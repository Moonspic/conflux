{
   "name": "functions",
   "description": "Cloud Functions for Firebase",
   "scripts": {
      "use:prod": "firebase use prod",
      "copy:prod": "cp services/firebase/service-account_prod.json services/firebase/service-account.json",
      "set:prod": "firebase functions:config:set app.env=prod app.jwtsecret=d79cb3841b7a4a1481b7274ebb09de31 algolia.adminkey=13607cada5efda16483bdfe4c324e4b6 algolia.index=prod algolia.appid=2EA38RAMYQ sendgrid.apikey=SG.9NkBWUiATU6yznw2P899WQ.CSUiGW8wEIUEYOseCdQQAMikGe6JicFUWtCGbnHBGDs",
      "firebase:prod": "firebase deploy -P prod",
      "deploy:prod": "npm-run-all use:prod set:prod firebase:prod",
      "logs": "firebase functions:log",
      "use:dev": "firebase use dev",
      "copy:dev": "cp services/firebase/service-account_dev.json services/firebase/service-account.json",
      "set:runtime": "firebase functions:config:get > .runtimeconfig.json",
      "set:dev": "firebase functions:config:set app.env=dev app.jwtsecret=d79cb3841b7a4a1481b7274ebb09de31 algolia.adminkey=13607cada5efda16483bdfe4c324e4b6 algolia.index=dev algolia.appid=2EA38RAMYQ sendgrid.apikey=SG.9NkBWUiATU6yznw2P899WQ.CSUiGW8wEIUEYOseCdQQAMikGe6JicFUWtCGbnHBGDs",
      "start:local": "firebase emulators:start",
      "start:dev": "firebase emulators:start --only functions",
      "firebase:dev": "firebase deploy -P dev",
      "serve:local": "npm-run-all use:dev set:dev set:runtime start:local",
      "serve:dev": "npm-run-all use:dev copy:dev set:dev set:runtime start:dev",
      "deploy:dev": "npm-run-all use:dev set:dev firebase:dev",
      "shell:local": "npm-run-all use:dev copy:dev set:local set:runtime run:shell",
      "run:shell": "npm run shell"
   },
   "engines": {
      "node": "12"
   },
   "main": "index.js",
   "dependencies": {
      "@sendgrid/mail": "^7.4.2",
      "algoliasearch": "^4.8.6",
      "axios": "^0.21.1",
      "cors": "^2.8.5",
      "crypto": "^1.0.1",
      "crypto-js": "^4.0.0",
      "crypto-random-string": "^3.3.1",
      "date-fns": "^2.19.0",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "firebase-admin": "^9.4.1",
      "firebase-functions": "^3.11.0",
      "jsonwebtoken": "^8.5.1",
      "jwt-decode": "^3.1.2",
      "morgan": "^1.10.0",
      "uid": "^2.0.0"
   },
   "devDependencies": {
      "eslint": "^7.22.0",
      "eslint-config-prettier": "^8.1.0",
      "eslint-plugin-prettier": "^3.3.1",
      "firebase-functions-test": "^0.2.0",
      "npm-run-all": "^4.1.5",
      "prettier": "^2.2.1"
   },
   "private": true
}
